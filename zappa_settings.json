{
  "prod": {
    "app_function": "scoutingbot.app",
    "aws_region": "us-east-1",
    "exclude": [
      "__pycache__",
      ".git/*",
      ".gitignore",
      ".python-version",
      "LICENSE",
      "README.md",
      "requirements.txt",
      "zappa_settings.json"
    ],
    "keep_warm": true,
    "keep_warm_expression": "rate(5 minutes)",
    "memory_size": 128,
    "profile_name": "default",
    "project_name": "scoutingbot",
    "runtime": "python3.7",
    "s3_bucket": "slackscoutingbot",
    "timeout_seconds": 60
  }
}